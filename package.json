{
  "name": "@fireenjin/editor",
  "namespace": "editor",
  "version": "1.0.0",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "es2015": "./dist/esm/index.js",
  "es2017": "./dist/esm/index.js",
  "jsnext:main": "./dist/esm/index.mjs",
  "types": "./dist/types/components.d.ts",
  "collection": "./dist/collection/collection-manifest.json",
  "scripts": {
    "copy:ace": "fireenjin copy node_modules/ace-builds/src-min-noconflict/ src/components/editor/ace-builds",
    "dev": " stencil build --docs --dev --watch --serve",
    "npm:install": "npm install",
    "patch": "node tasks/patch.js",
    "serve": "npm-run-all --parallel dev presets:watch",
    "start": "npm-run-all --parallel dev presets:watch",
    "new:component": "stencil generate",
    "build": "node --max_old_space_size=4096 ./node_modules/@stencil/core/bin/stencil build",
    "test": "stencil test --spec --e2e --coverage",
    "presets:build": "fireenjin presets",
    "presets:watch": "fireenjin presets watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "dependencies": {
    "@editorjs/editorjs": "^2.26.5",
    "@editorjs/embed": "^2.5.3",
    "@editorjs/header": "^2.7.0",
    "@editorjs/image": "^2.8.1",
    "@editorjs/list": "^1.8.0",
    "@editorjs/table": "^2.2.1",
    "@fireenjin/components": "^1.6.6",
    "@ionic-native/core": "^5.36.0",
    "@ionic-native/facebook": "^5.36.0",
    "@ionic-native/geolocation": "^5.36.0",
    "@ionic-native/google-plus": "^5.36.0",
    "@ionic-native/social-sharing": "^5.36.0",
    "@ionic-native/twitter-connect": "^5.36.0",
    "@monaco-editor/loader": "1.3.2",
    "@stencil/core": "3.2.0",
    "editorjs-change-case": "^0.0.3",
    "editorjs-paragraph-with-alignment": "^3.0.0",
    "editorjs-parser": "^1.5.3",
    "editorjs-style": "^3.0.3",
    "emmet-monaco-es": "^5.2.1",
    "handlebars": "^4.7.7",
    "json-logic-js": "^2.0.2",
    "monaco-editor": "^0.36.1",
    "remark": "^14.0.2",
    "typescript-debounce-decorator": "^0.0.18"
  },
  "devDependencies": {
    "@fireenjin/cli": "^1.1.5",
    "@fireenjin/docs": "^1.0.18",
    "@types/jest": "^29.5.0",
    "fs-extra": "^11.1.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-cli": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-typescript": "^1.0.1",
    "typescript": "^5.0.2"
  },
  "jest": {
    "preset": "@stencil/core/testing"
  }
}